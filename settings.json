{
    "editor.fontSize": 16,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Atom One Dark",
    "code-runner.executorMap": {
        "go": "go run",
        "perl": "perl",
        "javascript": "node",

        "python": "cd $dir && echo 'Running...' && /opt/homebrew/bin/timeout 8 python3 '$fileName' < /Users/sed/Documents/input_output/input.txt > /Users/sed/Documents/input_output/output.txt && echo 'Completed!!'",
        "java": "cd $dir && javac '$fileName' && echo 'Running...' && /opt/homebrew/bin/timeout 5 java '$fileNameWithoutExt' < /Users/sed/Documents/input_output/input.txt > /Users/sed/Documents/input_output/output.txt && echo 'Completed!!'",
        "c": "cd $dir && /opt/homebrew/bin/gcc --std=gnu17 '$fileName' && echo 'Running...' && /opt/homebrew/bin/timeout 4 ./a.out < /Users/sed/Documents/input_output/input.txt > /Users/sed/Documents/input_output/output.txt && echo 'Completed!!'",
        "cpp": "cd $dir && /opt/homebrew/bin/g++ --std=gnu++17 '$fileName' && echo 'Running...' && /opt/homebrew/bin/timeout 4 ./a.out < /Users/sed/Documents/input_output/input.txt > /Users/sed/Documents/input_output/output.txt && echo 'Completed!!'"
    },
    "editor.formatOnSave": true,
    "files.autoSave": "onFocusChange",
    "notebook.output.textLineLimit": 500,
    "code-runner.saveFileBeforeRun": true,
    "debug.javascript.resourceRequestOptions": {},

    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.default.compilerPath": "/opt/homebrew/bin/g++ --std=gnu++17"
}